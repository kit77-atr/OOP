import java.util.Scanner;

class Player {
    // 1. Attribute เป็น private ชื่อ score (int)
    private int score;

    // 2. Constructor รับค่า initialScore มากำหนดค่าเริ่มต้น
    public Player(int initialScore) {
        this.score = initialScore;
    }

    // 3. เมธอด public int getScore() ทำหน้าที่ return ค่า score
    public int getScore() {
        return this.score;
    }

    // 4. เมธอด public void setScore(int newScore) ทำหน้าที่รับค่าใหม่มาอัปเดต
    public void setScore(int newScore) {
        this.score = newScore;
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // รับค่า "คะแนนเริ่มต้น" (บรรทัดแรก)
        int initialInput = scanner.nextInt();

        // รับค่า "คะแนนที่อัปเดต" (บรรทัดที่สอง)
        int updatedInput = scanner.nextInt();

        // สร้างออบเจกต์ Player ด้วย "คะแนนเริ่มต้น"
        Player myPlayer = new Player(initialInput);

        // เรียกใช้เมธอด setScore() เพื่ออัปเดตคะแนนด้วย "คะแนนที่อัปเดต"
        myPlayer.setScore(updatedInput);

        // แสดงผลคะแนนล่าสุด โดยเรียกใช้เมธอด getScore()
        System.out.println(myPlayer.getScore());
        
        scanner.close();
    }
